-->
<table>
    <column>
        <name>idfont</name>
        <type>serial</type>
        <null>NO</null>
    </column>
    <column>
        <name>lastmod</name>
        <type>date</type>
    </column>
    <column>
        <name>name</name>
        <type>character varying(100)</type>
        <null>NO</null>
    </column>
    <constraint>
        <name>webcreator_blocks_pkey</name>
        <type>PRIMARY KEY (idfont)</type>
    </constraint>
    <constraint>
        <name>uniq_name_webcreator_blocks</name>
        <type>UNIQUE (name)</type>
    </constraint>
</table>