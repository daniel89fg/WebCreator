<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : webfooter.xml
    Author     : Athos Online
    Description:
        Structure for the webfooters table.
-->
<table>
    <column>
        <name>content</name>
        <type>longblob</type>
    </column>
    <column>
        <name>idfooter</name>
        <type>serial</type>
        <null>NO</null>
    </column>
    <column>
        <name>lastmod</name>
        <type>date</type>
    </column>
    <column>
        <name>name</name>
        <type>character varying(100)</type>
        <null>NO</null>
    </column>
    <column>
        <name>properties</name>
        <type>LONGBLOB</type>
    </column>
    <constraint>
        <name>webfooters_pkey</name>
        <type>PRIMARY KEY (idfooter)</type>
    </constraint>
</table>