<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : webheader.xml
    Author     : Athos Online
    Description:
        Structure for the webheaders table.
-->
<table>
    <column>
        <name>content</name>
        <type>longblob</type>
    </column>
    <column>
        <name>idheader</name>
        <type>serial</type>
        <null>NO</null>
    </column>
    <column>
        <name>lastmod</name>
        <type>date</type>
    </column>
    <column>
        <name>name</name>
        <type>character varying(100)</type>
        <null>NO</null>
    </column>
    <column>
        <name>properties</name>
        <type>longblob</type>
    </column>
    <constraint>
        <name>webheaders_pkey</name>
        <type>PRIMARY KEY (idheader)</type>
    </constraint>
</table>