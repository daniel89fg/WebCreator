{% macro includeView(webIncludes, templateVars, location) %}
    {% for file in webIncludes %}
        {% set aux = file|split('_') %}

        {% if (aux[1] == location ~ '.html.twig') %}
            {% include['WebCreator/Include/' ~ file, ''] with templateVars %}
        {% endif %}
    {% endfor %}
{% endmacro %}