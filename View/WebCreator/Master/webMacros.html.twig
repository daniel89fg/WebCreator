{% macro includeView(i18n, fsc, webIncludes, location) %}
    {% for file in webIncludes %}
        {% set aux = file|split('_') %}
        
        {% if (aux[1] == location ~ '.html.twig') %}
            {% include['WebCreator/Include/' ~ file, ''] %}
        {% endif %}
    {% endfor %}
{% endmacro %}